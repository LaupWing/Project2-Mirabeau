<section class="room centering">
    <h2 class="<%=room.occupancy%>"><%= room.roomName %></h2>
    <% keys.forEach((key,index)=>{ %>
        <% if(index === 0){ %>
            <input name="info<%=roomID%>" type="radio" id="<%=key%><%= roomID %>" checked>
        <% } else {%>
            <input name="info<%=roomID%>" type="radio" id="<%=key%><%= roomID %>">
        <% } %>
    <% }) %>
    <section class="currentInfo-container centering">
        <div class="currentInfo temperature centering">
            <p>Temperature</p>
            <h3><%= room.temperature %> c&deg</h3>
        </div>
        <div class="currentInfo soundLevel centering">
            <p>Sound Level</p>
            <h3><%= room.soundLevel %></h3>
        </div>
        <div class="currentInfo ambientLight centering">
            <p>Ambient Light</p>
            <h3><%= room.ambientLight %></h3>
        </div>
        <div class="currentInfo co2 centering">
            <p>CO2</p>
            <h3><%= room.co2 %></h3>
        </div>
        <div class="currentInfo humidity centering">
            <p>Humidty</p>
            <h3><%= room.humidity %></h3>
        </div>
    </section>
    <nav>
        <% keys.forEach(key=>{ %>
            <label class="<%=key%> centering" for="<%=key%><%= roomID %>">
                <img src="./img/navitems/<%= key %>.png" alt="">
            </label>
        <% }) %>
    </nav>
</section>